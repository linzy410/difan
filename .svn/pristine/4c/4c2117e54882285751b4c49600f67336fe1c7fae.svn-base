@charset "utf-8";
/* CSS Document */

#main{width:980px;margin:0 auto;}
#site-mast{background:#fff}

.page-nav{display:inline;white-space:nowrap;font-size:14px;zoom:1}
.page-nav li{display:inline;margin-left:5px;vertical-align:top}
.page-nav a{display:inline-block;width:34px;height:24px;padding:5px 0;border:1px solid #e3e8e8;border-radius:1px;zoom:1;line-height:24px;background:#FFF;color:#F08337}
.page-nav a:hover{text-decoration:none;border-color:#F08337}
.page-nav .current a{color:#FFF;font-weight:bold;border-color:#F08337;background:#F08337}
.page-nav .previous a{width:42px;padding:5px 10px 5px 16px;background:#fff url("../images/newicon.png") no-repeat 0 -111px}
.page-nav .next a{width:42px;padding:5px 16px 5px 10px;background:#fff url("../images/newicon.png") no-repeat -56px -111px}
#content{float:left;width:720px;_display:inline;padding:0}
#content .mainbox{background:#FFF;border:1px solid #d4d4d4;padding:20px;clear:both;zoom:1;height:auto!important;min-height:400px;height:400px}
#content .mainbox h2{font-size:28px;border-bottom:1px solid #dcdcdc;margin-bottom:10px;padding-bottom:10px}
#content .mainbox h2 span{font-size:14px}
#content .mainbox h2 span.charge{padding-left:10px;font-weight:normal}
#content .mainbox h2 span.op-area{position:absolute;top:40px;right:20px;font-size:14px;font-weight:normal}
#content .mainbox p.tip{margin-bottom:5px}
#content .result-box-wrapper{margin:30px auto;text-align:center}

#content .head{padding:0 10px 10px}
#content .head h2{display:inline;padding-left:10px;font-size:28px}
#content .head .filter{position:absolute;top:25px;right:10px}
#content .head .filter li{float:left;margin:0 5px 0 0;font-size:12px}
#content .head .filter a{padding:3px 1px}
#content .head .filter .current a{padding:3px 7px;background:#00adb2;color:#fff;-moz-border-radius:2px;-webkit-border-radius:2px}
#content .title{padding:0 10px}
#content .title h3{display:inline;padding-left:10px;font-size:20px}

.common-rating,.common-rating .rate-stars{display:inline-block;*zoom:1;height:12px;line-height:12px;background:url("../images/index.png");background-repeat:repeat-x;background-position:0 0;overflow:hidden;vertical-align:top}
.common-rating{width:60px;background-position:0 -17px}
.reco-sliders-w .head .trigger-container{background:#fff;left: auto;right: 0;top: 8px;width: auto;}
.mt-slider-trigger{cursor:pointer;display: inline-block;height: 11px;margin-left: 10px;margin-top: 5px;position: relative;top: 0;width: 11px;}
.mt-slider-trigger-container{ background:#CEDCDD;bottom: -20px;font-size: 0;height: 20px;left: 0;position: absolute;text-align: center;width: 100%;}
.ccf{overflow:hidden;_zoom:1}

.deal-tile:hover .short-title{color:#333}
.deal-tile .w-link:hover .short-title{_color:#333}
.deal-tile .num{font-family:arial,sans-serif}
.deal-tile .deal-mark{top:-4px;left:10px}

.deal-tile__cover img{display:block}
.deal-tile__cover .trade-geo{position:absolute;bottom:0;width:318px;padding:5px 16px;max-height:166px;z-index:5;font-size:12px;line-height:22px;color:#ccc;opacity:0;filter:Alpha(opacity=0);overflow:hidden;background-color:#000;cursor:pointer;text-decoration:none!important;-moz-transition:opacity .5s ease-out;-webkit-transition:opacity .5s ease-out;transition:opacity .5s ease-out}
.deal-tile__cover .trade-geo em{color:#f76120}
.deal-tile__cover .geo-hover{opacity:.9;filter:Alpha(opacity=90)}
.deal-tile__cover .trade-geo:hover{opacity:.9;filter:Alpha(opacity=90)}
.deal-tile__cover .geo-title{font-weight:bold}
.deal-tile__cover .geo-title__icon{display:inline-block;width:13px;height:13px;padding-right:2px;vertical-align:-2px;*vertical-align:baseline;background:url("../images/label-icons.png") 0 0 no-repeat;}
.deal-tile__cover .deal-st--expire{background-position:0 -224px}
.deal-tile__title{margin-top:15px;padding:0 16px;font-family:'宋体',sans-serif}
.deal-tile__title .w-link{display:block;height:64px;overflow:hidden;_cursor:pointer;-moz-transition:color .2s;transition:color .2s}
.deal-tile__title .w-link:hover{text-decoration:none}
.deal-tile__title .xtitle,.deal-tile__title .short-title{display:block;-webkit-transition:color .2s;transition:color .2s}
.deal-tile__title .xtitle{margin-bottom:4px;color:#333;font-size:16px;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;width:306px}
.deal-tile__title .short-title{line-height:18px;color:#999;font-weight:normal}
.deal-tile__title em{font-weight:inherit;color:#f76120}

.deal-tile__detail .price{margin-right:10px;font-size:26px;color:#f76120}
.deal-tile__detail .price .price-label--wedding{font-size:16px;margin-right:6px}
.deal-tile__detail .value{font-size:12px}
.deal-tile__detail .value del,.deal-tile__detail .value .num--wedding{padding-left:6px}
.deal-tile__extra{padding:1px;border-top:1px #f3f3f3 solid}
.deal-tile__extra .extra-inner{height:33px;padding:0 15px;font-size:12px;background-color:#fafafa}
.deal-tile__extra .sales{float:left;height:33px;line-height:33px}
.deal-tile__extra .sales .num{margin-left:2px;color:#f76120;font-weight:normal}
.deal-tile__extra .rate-info{float:right;height:33px;line-height:33px;cursor:pointer}
.deal-tile__extra .rate-info:hover,.deal-tile__extra .rate-info:hover .rate-info__count{color:#F08337;text-decoration:none}
.deal-tile__extra .rate-info__bar{margin-right:5px;*margin-right:8px;vertical-align:-1px;*vertical-align:1px}
.deal-tile__extra .rate-info__count{display:inline-block;color:#949494}
.deal-tile__extra .rate-info--noreviews{cursor:default}
.deal-tile__extra .rate-info--noreviews:hover{color:inherit}
.deal-tile__extra .sp-around-icon{background-position:0 -62px}
.deal-tile__extra .subway-distance,.deal-tile__extra .around-distance{float:right;padding-left:5px;font-size:12px;line-height:33px}
.deal-tile__extra .around-distance em{padding:0 3px;font-weight:bold;color:#fd6b44}


.mall{float:left;width:720px;}
.mall .brand-tile,.mall .deal-tile{margin-bottom:16px;box-shadow:0 2px 0 0 #ededed}
.mall .brand-tile:hover,.mall .deal-tile:hover{border-bottom-color:#F08337;box-shadow:inset 0 -1px 0 0 #F08337}
.mall .deal-tile--even{float:right}
.mall .page-nav-wrapper{clear:both;padding-top:20px;text-align:center}
.pg-nav .page-nav-wrapper{padding-top:24px}
.mall--3cols{width:980px}
.mall--3cols .deal-tile,.mall--3cols .brand-tile{margin:0 16px 16px 0;width:314px}
.mall--3cols .brand-tile--even,.mall--3cols .deal-tile--even{float:left;margin-right:0}
.mall--3cols .deal-tile__cover .trade-geo{width:282px}
.mall--3cols .deal-tile__cover .deal-st{width:314px;height:192px;background-position:-18px -11px}
.mall--3cols .deal-tile__cover .deal-st--expire{background-position:-18px -235px}
.mall--3cols .deal-tile__title .xtitle{_width:282px}
.mall--3cols .deal-tile__detail{padding:4px 16px}
.mall--3cols .deal-tile__detail .price{font-size:22px}

/* 本周精选 */

.site-fs{float:right;width:764px;height:479px;position:relative;background:#fff;border-bottom:1px solid #ddd;border-right:1px solid #ddd}
.site-fs__cell{margin:0 15px;padding:15px 0;zoom:1}
.site-fs__cell-small{padding:12px 0}
.site-fs__cell .label{padding-left:20px;line-height:24px;color:#999;font-size:12px;font-weight:normal;*zoom:1}
.site-fs__cell-small .label{float:left;margin-right:20px}
.site-fs__cell--slider{padding-bottom:0}
.site-fs__cell .label i{position:absolute;left:15px;width:14px;height:18px;background:url("../images/newicon.png") no-repeat}
.site-fs__cell--geo .label i{background-position:-83px -58px}
.site-fs__cell--hot .label i{background-position:-115px -58px}
.site-fs__cell--area .label i{background-position:-131px -58px}
.filter-strip{margin-left:20px;height:24px;overflow:hidden;font-size:12px;line-height:24px}
.filter-strip__list li{display:inline-block;*display:inline;*zoom:1;margin-right:20px;vertical-align:top}
.filter-strip__list a{color:#666}
.filter-strip__list .hot{color:#f76120}
.filter-strip__all{position:relative;float:right;border:1px solid #ededed;margin:1px 0;padding:0 13px 0 5px;color:#666;line-height:20px}
.filter-strip__all .tri{border-top:3px solid #666;top:9px;right:3px}
.filter-strip__all:hover,.filter-strip__list a:hover{color:#F08337;text-decoration:none}
.filter-strip__all:hover .tri{border-top-color:#F08337}


.reco-sliders-w{position:relative;padding-right:1px;overflow:visible!important;zoom:1}
.reco-sliders-w .left{position:relative;float:left;width:367px}
.reco-sliders-w .right{float:right;width:366px;position:relative}
.reco-sliders-w .link{display:block}
.reco-sliders-w .link:hover{text-decoration:none;color:#F08337}
.reco-sliders-w .discount{position:absolute;right:10px;top:0;width:28px;height:37px;padding:13px 22px 0 12px;color:#FFF;font-weight:bold;font-family:Arial;font-size:18px;text-align:center;background:url("../images/label-icons.png") 0 -20px no-repeat;z-index:2}
.reco-sliders-w .discount-big{width:62px;padding:8px 0 5px;font-size:24px}
.reco-sliders-w .img{vertical-align:top}
.reco-sliders-w .title{float:left;width:240px;height:40px;padding:5px 10px;}
.reco-sliders-w .title--banner{width:346px}
.reco-sliders-w .left .title--banner{border-right:1px solid #d6d6d6}
.reco-sliders-w .one-pic-title{width:610px}
.reco-sliders-w .desc,.reco-sliders-w .xtitle,.reco-sliders-w .price{width:245px;overflow:hidden;text-overflow:ellipsis;white-space:nowrap}
.reco-sliders-w .desc{font-size:12px;color:#999}
.reco-sliders-w .xtitle{color:#666}
.reco-sliders-w .price{float:right;width:100px;height:50px;line-height:50px;text-align:center;color:#f76120;font-weight:bold;font-size:24px;font-family:arial}
.reco-sliders-w .head{padding:0 0 5px 20px}
.reco-sliders-w .head .icon{position:absolute;top:2px;left:0;width:15px;height:15px;background:url("../images/newicon.png") no-repeat -97px -62px;*display:inline;zoom:1}
.reco-sliders-w .head h2{margin-bottom:8px;line-height:20px;font-size:14px;color:#666}
.reco-sliders-w .split-line{position:absolute;right:-10px;top:0;width:10px;height:50px;}
.reco-sliders-w .split-side{position:absolute;bottom:-5px;width:8px;height:60px;}
.reco-sliders-w .split-side--left{left:-8px;background-position:-144px 0}
.reco-sliders-w .split-side--right{right:-8px;background-position:-136px 0}
.reco-sliders-w .slide__item--banner .split-side--right{background-position:-128px 0}
.reco-sliders-w .mt-slider-sheet-container{overflow:visible!important;height:270px!important}
.reco-sliders-w .mt-slider-sheet{width:100%}
.reco-sliders-w .head .mt-slider-trigger,.reco-sliders-w .head .mt-slider-current-trigger{background: url("../images/slide.png") no-repeat scroll -14px -77px rgba(0, 0, 0, 0);margin-left: 7px;}
.reco-sliders-w .head .mt-slider-current-trigger{background-position:0 -77px}
.reco-sliders-w .pre-next .mt-slider-previous,.reco-sliders-w .pre-next .mt-slider-next{position:absolute;right:-10px;top:110px;width:42px;height:50px;background:url("../images/slide.png") no-repeat -248px 0;z-index:2;cursor:pointer}
.reco-sliders-w .pre-next .mt-slider-previous{left:-10px;right:auto;background-position:-206px 0}

/* 团购精选 */
.site-ms{padding:20px 0 40px;width:980px;float:left;overflow:hidden;_zoom:1}
.mall-branding{padding:0 0 17px 0;clear:both;font-size:18px;color:#666;_zoom:1}
.mall-branding i{float:Left;width:23px;height:25px;background:url("../images/icon.png") no-repeat -111px -59px;padding-right:10px}

/* 城市团购 */
.holy-reco{width:980px;margin:0 auto;padding-bottom:20px;_display:none;float:left;}
.holy-reco .current{background-color:#FFF;border-top:3px solid #F08337;height:41px;line-height:41px;margin-top:-1px}
.holy-reco .current .tab-item{font-weight:bold;color:#333}
.holy-reco .content-navbar{width:978px;height:41px;border-bottom:1px solid #DDD;background:#fbfbfb;*border-bottom:1px solid #dfdfdf;filter:progid:DXImageTransform.Microsoft.gradient(gradientType=0,startColorstr='#FFFEFEFE',endColorstr='#FFFAFAFA');background-image:-webkit-gradient(linear,50% 0,50% 100%,color-stop(0%,#fefefe),color-stop(100%,#fafafa));background-image:-webkit-linear-gradient(top,#fefefe,#fafafa);background-image:-moz-linear-gradient(top,#fefefe,#fafafa);background-image:-o-linear-gradient(top,#fefefe,#fafafa);background-image:linear-gradient(top,#fefefe,#fafafa)}
.holy-reco__content{border:1px solid #DDD;padding:10px;background:#FFF}
.holy-reco__content a{display:inline-block;color:#3e4545;font-size:12px;padding:0 10px;line-height:16px;white-space:nowrap}
.content-navbar{background-color:#fff;border-color:#ddd;border-image: none;border-style:solid solid none;border-width:1px 1px 0;bottom:-1px;height:43px;width:958px;}
.content-navbar li{float: left;}
.content-navbar .tab-item{border-left:1px solid rgba(0, 0, 0, 0);border-right:1px solid #E0E0E0;box-shadow:0 0 1px #fff inset;color:#F08337;display:block;height:40px;line-height:40px;outline:0 none;padding:0 15px;text-decoration: none;}

/* loading */
.deals-async-loading{margin-bottom:16px;padding:10px 0;height:21px;background-color:#FFF;border:1px solid #ddd;float:left;width:100%}
.deals-async-loading__tips{margin:0 auto;width:90px;font-size:14px;color:#999}
.deals-async-loading__tips i{float:right;width:44px;height:21px;background-image:url(../images/loading.gif);background-position:center center;background-repeat:no-repeat}
