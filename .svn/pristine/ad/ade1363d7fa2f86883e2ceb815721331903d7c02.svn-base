@charset "utf-8";
/* CSS Document */

.common-banner--newtop {border:0 none;height:122px;margin:2px auto 0;overflow:hidden;padding:0;width:980px;position:relative}
.common-banner .close {position:absolute;right:6px;top:6px;}
.common-close--small {background-position:0 -30px;height:8px;width:8px;}
.common-close {background-position:0 0;height:13px;overflow:hidden;text-indent:100px;width:13px;}
.close-sprite, .common-close, .common-close:hover, .common-close--small, .common-close--small:hover{background:url("../images/close.png")}

/* 头部*/
.site-mast,.new-header{background:#FFF;min-width:980px}
.site-mast__user-nav-w{background:#fbfbfb;height:28px;font-size:12px;*border-bottom:1px solid #dfdfdf;border-top:1px solid #EEE;filter:progid:DXImageTransform.Microsoft.gradient(gradientType=0,startColorstr='#FFFEFEFE',endColorstr='#FFFAFAFA');background-image:background-image:-moz-linear-gradient(top,#fefefe,#fafafa);background-image:-o-linear-gradient(top,#fefefe,#fafafa);background-image:linear-gradient(top,#fefefe,#fafafa);-moz-box-shadow:0 0 0 1px #FFF,0 0 0 2px #dfdfdf,0 0 5px 3px #f0f0f0;box-shadow:0 0 0 1px #fff,0 0 0 2px #dfdfdf,0 0 5px 3px #f0f0f0}

.site-mast__user-nav{position:relative;*zoom:1;margin:0 auto;width:980px;z-index:90}
.site-mast__separator{float:left;height:28px;vertical-align:top}
.site-mast__separator--left{border-right:1px solid #eee}
.site-mast__separator--right{border-left:1px solid #fff}
.site-mast__user-nav .icon{display:inline-block;*display:inline;*zoom:1;margin-right:3px;width:15px;height:15px;line-height:15px;vertical-align:text-top;background:url("../images/newicon.png") left top no-repeat}
.site-mast__user-nav .icon-mobile{background-position:0 -76px}
.site-mast__user-nav .icon-movie{background-position:-18px -76px}
.site-mast__user-nav .icon-cart{background-position:-35px -76px}
.site-mast .mobile-info{float:left}
.mobile-info__item{float:left;border-left:1px solid #FFF;border-right:1px solid #EEE}
.mobile-info__link{display:inline-block;*display:inline;*zoom:1;padding:6px 8px;width:89px;height:16px;color:#999;text-align:center;vertical-align:top}
.mobile-info__link:hover{color:#F08337;text-decoration:none}
.mobile-info__link:hover .icon-mobile{background-position:0 -62px}
.mobile-info__link:hover .icon-movie{background-position:-18px -62px}

/* 用户登陆 */
.site-mast__user-w{float:right;_line-height:28px}
.site-mast__user-w .site-mast__separator--left{box-shadow:1px 0 1px #fff}
.site-mast .user-info{float:left;display:inline-block;*display:inline;*zoom:1;height:16px;padding:6px 8px;_padding:0;color:#666;vertical-align:top}
.user-info__moto{color:#999}
.user-info__login{padding:0 10px;color:#f76120;border-right:1px solid #DDD}
.user-info__signup{padding:0 10px;color:#666}
.user-info__logout{padding:0 0 0 10px;color:#666}
.user-info__logout:hover{color:#F08337}
.user-info__name{padding-right:10px;_padding:8px;z-index:1}

/* 下拉菜单 */
.site-mast .dropdown{float:left;position:relative;vertical-align:top}
.site-mast .dropdown__toggle{position:relative;display:inline-block;height:16px;padding:6px 20px 6px 12px;margin:0 1px 0;color:#666;z-index:93;_padding:0 20px 0 12px;*border-left:1px solid #fff;*margin-left:0;*display:inline;*zoom:1}
.site-mast .dropdown__toggle .vertical-bar{position:absolute;right:-2px;top:11px;display:block;*display:none;height:12px;width:1px;background:#ddd;z-index:94}
.site-mast .dropdown--first .dropdown__toggle{border-left:1px solid #fff;margin-left:0}
.site-mast .tri--dropdown{top:14px;right:7px;border-top:3px solid #999}
.site-mast .dropdown--cart{*z-index:1}
.site-mast .dropdown--cart .dropdown__toggle{_padding:0 20px}
.site-mast .dropdown--cart .icon-cart{_position:absolute;_left:5px}
.site-mast .dropdown--cart .cart-count{margin:0 .1em;font-weight:normal;color:#e95002}
.site-mast .dropdown--account{*z-index:1}
.site-mast .dropdown--account:hover #J-my-account-menu,.site-mast  #J-site-merchant:hover .dropdown-menu--merchant,.site-mast  #J-site-help:hover .dropdown-menu--help, .site-mast  #J-my-more:hover #J-my-more-menu{display:block;}
.site-mast .dropdown--msg .dropdown-menu--msglist{width:285px;padding-top:0;padding-bottom:40px;line-height:20px;zoom:1}
.site-mast .dropdown--msg .common-badge{position:absolute;top:0;right:9px}
.site-mast .dropdown-menu{display:none;position:absolute;top:27px;right:0;padding:10px 0;border:1px solid #ddd;color:#666;background:#fff;z-index:92;box-shadow:1px 1px 2px rgba(203,203,203,0.5)}
.site-mast .dropdown-menu--empty{line-height:50px;text-align:center}
.site-mast .dropdown-menu--msglist{text-align:center;padding:10px 0;border-bottom:1px solid #d3d3d3}
.site-mast .dropdown-menu--msglist .no-msg{text-align:center;padding:10px 0;border-bottom:1px solid #d3d3d3}
.site-mast .dropdown-menu--msglist ul{*position:relative;border-bottom:1px solid #d3d3d3;max-height:250px;_height:250px;overflow:hidden;zoom:1}
.site-mast .dropdown-menu--msglist li:hover{cursor:pointer}
.site-mast .dropdown-menu--msglist li{position:relative;border-bottom:1px dotted #d3d3d3;padding:10px 20px;text-align:left}
.site-mast .dropdown-menu--msglist li a{color:#666}
.site-mast .dropdown-menu--msglist li:last-child{border-bottom:0}
.site-mast .dropdown-menu--msglist .common-close{position:absolute;top:16px;right:10px;display:none}
.site-mast .dropdown-menu--msglist .isread a,.site-mast .dropdown-menu--msglist .isread{color:#999}
.site-mast .dropdown-menu--msglist .current{background-color:#f8f8f8}
.site-mast .dropdown-menu--msglist .current a{color:#2bb8aa}
.site-mast .dropdown-menu--msglist .current .common-close{display:block}
.site-mast .dropdown-menu--msglist .view-all{position:absolute;bottom:10px;right:20px;text-align:right;color:#999}
.site-mast .dropdown-menu--msglist .scroll-bar{position:absolute;opacity:.5;background-color:#000;width:6px;border-radius:3px;cursor:default;transition:opacity .3s;overflow:hidden}
.site-mast .dropdown-menu--msglist .msglist{position:absolute;left:0;top:0}
.site-mast .dropdown--open .dropdown__toggle{margin:0;text-decoration:none;background:#fff;color:#F08337;border-left:1px solid #ddd;border-right:1px solid #ddd;box-shadow:1px -1px 2px rgba(203,203,203,0.5)}
.site-mast .dropdown--open .vertical-bar{display:none}
.site-mast .dropdown--open .tri--dropdown{top:11px;border-top:3px dashed transparent;border-bottom:3px solid #2db8ad}
.site-mast .dropdown--open .dropdown-menu{display:block}
.site-mast .dropdown-menu--text .dropdown-menu__item{display:block;padding:0 12px;height:30px;line-height:30px;text-decoration:none;background:#fff;color:#666;transition:background .2s ease-in}
.site-mast .dropdown-menu--text .dropdown-menu__item:hover{color:#F08337;background:#f8f8f8}
.site-mast .dropdown-menu--deal .dropdown-menu__item{padding:10px 13px;border-bottom:1px dotted #EEE;height:55px;overflow:hidden;color:#666;transition:background .2s ease-in}
.site-mast .dropdown-menu--deal .dropdown-menu__item:hover{background:#f8f8f8}
.site-mast .dropdown-menu--deal .dropdown-menu__item .deal-link{padding:0;color:#666}
.site-mast .dropdown-menu--deal .dropdown-menu__item .deal-link:hover{color:#F08337;text-decoration:none}
.site-mast .dropdown-menu--deal .dropdown-menu__item .deal-title{height:32px;margin-bottom:4px;line-height:16px;overflow:hidden;font-weight:normal}
.site-mast .dropdown-menu--deal .dropdown-menu__item .deal-cover{float:left;margin:2px 10px 0 0}
.site-mast .dropdown-menu--deal .dropdown-menu__item .deal-price-w{height:18px;display:block;line-height:18px;text-overflow:ellipsis;white-space:nowrap;_white-space:normal;overflow:hidden}
.site-mast .dropdown-menu--deal .dropdown-menu__item .deal-price{padding-right:10px;font:bold 12px/12px arial,sans-serif;color:#ee5238}
.site-mast .dropdown-menu--account{width:80px}
.site-mast .dropdown-menu--account .amount{font-size:.75em;color:#e95002}
.site-mast .dropdown-menu--history{width:219px}
.site-mast .dropdown-menu--history .deal-price-w:hover{color:#666;text-decoration:none}
.site-mast .dropdown-menu--history .old-price{text-decoration:line-through}
.site-mast .dropdown-menu--history .clear{height:30px;line-height:40px;text-align:center}
.site-mast .dropdown-menu--history .clear__btn{color:#F08337}
.site-mast .dropdown-menu--history .clear__btn:hover{text-decoration:underline}
.site-mast .dropdown-menu--cart{width:219px}
.site-mast .dropdown-menu--cart .delete{float:right}
.site-mast .dropdown-menu--cart .list-wrapper{margin-bottom:20px}
.site-mast .dropdown-menu--cart .long-list{height:380px;overflow:hidden}
.site-mast .dropdown-menu--cart .cart-discription{margin:10px 0;text-align:center}
.site-mast .dropdown-menu--cart .check-my-cart{margin-bottom:16px;text-align:center}
.site-mast .dropdown-menu--cart .cart-deal-num{margin:0 3px;font-weight:bold}
.site-mast .dropdown-menu--more{width:80px}
.site-mast .dropdown-menu--help{width:92px}
.site-mast .dropdown-menu--merchant{width:97px}
.dropdown__toggle--merchant:hover,.dropdown__toggle--my-more:hover{text-decoration:none}
.site-mast .dropdown--history{float:right}
.site-mast .dropdown--history .dropdown__toggle{line-height:24px;height:24px;margin:0;_margin-top:1px;padding:8px 25px 8px 15px;border-left:1px solid #179a8f;border-right:1px solid #179a8f;font-size:12px;color:#FFF;*display:inline;*zoom:1}
.site-mast .dropdown--history .tri--dropdown{top:18px;right:8px;border-top:3px solid #fff}
.site-mast .dropdown-menu--history{top:40px;width:219px;font-size:12px}
.site-mast .dropdown--open-history .dropdown__toggle{_margin-top:0;border-top:1px solid #DDD;color:#F08337;box-shadow:none}
.site-mast .dropdown--open-history .tri--dropdown{top:15px;border-top:3px dashed transparent;border-bottom:3px solid #2db8ad}
.growth-info--nav{position:relative;overflow:visible;cursor:pointer}
.growth-info--nav .level-icon{margin-left:5px;position:absolute;top:11px;right:8px}
.site-mast__user-nav .growth-bubble{display:none;position:absolute;top:29px;right:0;padding:7px 25px 7px 20px;border:1px solid #f5d8a7;background-color:#fff6db;cursor:default;text-align:center;*width:450px}
.growth-info--open .growth-bubble{display:block}
.site-mast__user-nav .growth-info--nav p{line-height:1.5}
.site-mast__user-nav .growth-info--nav .level-icon{position:relative;top:-1px;left:0}
.site-mast__user-nav .growth-bubble{top:29px;right:0;padding:7px 25px 7px 20px;border:1px solid #f5d8a7;background-color:#fff6db}
.site-mast__user-nav .growth-bubble .bubble-text{float:left;margin-right:10px}
.site-mast__user-nav .growth-bubble .bubble-button{float:right;margin-top:0}
.site-mast__user-nav .growth-bubble .btn-hot{vertical-align:top}

/* logo */
.site-mast__branding{margin:0 auto;position:relative;width:980px;height:104px;}
.site-mast--mini .site-mast__branding{height:auto}
.site-mast__branding .site-logo{float:left;display:block;_display:inline;width:130px;height:60px;margin-top:30px;text-indent:-100px;overflow:hidden;background-position:0 -270px}
.site-mast--mini .site-mast__branding{margin:40px auto 30px}
.site-mast--mini .site-mast__branding .site-logo{margin:0}
.site-mast__branding .site-commitment{float:right;margin-top:38px}
.site-mast__branding .commitment-item{float:left;display:block;width:82px;height:50px;background-position:-164px 1px;border-right:1px dotted #d4d4d4}
.site-mast__branding .commitment-item--retire{background-position:-16px 1px}
.site-mast__branding .commitment-item--free{background-position:-90px 1px}
.site-mast__branding .commitment-item--expire{width:62px;background-position:-164px 1px;border-right:0}

/* 城市信息 */
.site-mast .city-info,.new-header .city-info{float:left;margin:40px 0 0 2px;width:90px}
.city-info__name{font-size:18px;font-weight:bold;color:#000}
.city-info__name:hover{text-decoration:none}
.city-info__toggle{font-size:12px;color:#C8C9CA}

/* 搜索建议 */
.search-suggest{position:absolute;width:311px;top:64px;left:0;margin-top:-1px;border:1px solid #d4d4d4;border-top:0;overflow:hidden;z-index:51;background:#FFF;cursor:pointer}
.search-suggest__tip{color:#999;padding:3px 7px;font-size:12px;cursor:default}
.search-suggest__item{padding:3px 7px}
.search-suggest__item .remove{display:none;float:right;font-size:12px;color:#999}
.search-suggest__item--history{color:#F08337}
.search-suggest__item--over{background:#def7f8}
.search-suggest__item--over .remove{display:block}
.search-suggest__item--count{float:right;margin-top:1px;font-size:12px;color:#999}
.site-mast__site-nav-w{background:#F08337}
.site-mast__site-nav-inner{position:relative;margin:0 auto;width:980px;height:40px;z-index:40}
.site-mast .navbar{margin-left:215px;padding-left:1px;_height:40px}
.site-mast .navbar--newheader{margin-left:0}
.navbar__item-w{float:left;margin-left:-1px}
.navbar__item{position:relative;display:inline;height:39px;font-weight:bold;color:#fff;}
.navbar__item .icon-new{top:-4px;right:10px}
.navbar .current .icon-new,.navbar__item:hover .icon-new{right:9px}
.site-body{margin:0 auto;width:980px}
.mt-cates{position:absolute;top:0;width:170px;height:28px;padding-left:36px;line-height:28px;cursor:default;text-align:left;color:#999;font-weight:bold;border-bottom:0;background:#fff;border:6px solid #F08338;}
.mt-cates b{position:absolute;background:url("../images/newicon.png") no-repeat;width:13px;height:8px;top:10px;right:30px}
.mt-cates i{position:absolute;width:20px;height:20px;top:9px;left:175px;}
.nav-unfolded i{background-position:-113px 0}
.cate-nav{position:absolute;top:50px;width:206px;height:465px;border:2px solid #F08337;overflow:visible;background:#fff;line-height:22px;font-size:12px;font-family:'宋体',sans-serif;z-index:1;box-shadow:2px 2px 5px #d9d9d9;left:5px;}
.cate-nav__item{position:relative}
.cate-nav__item:hover{*cursor:default}
.cate-nav__item a:hover{color:#F08337;text-decoration:none}
.cate-nav__item .cate-121{background-position:-45px 0}
.cate-nav__item .cate-122{background-position:-45px -96px}
.cate-nav__item .cate-6{background-position:-45px -20px}
.cate-nav__item .cate-38{background-position:-45px -38px}
.cate-nav__item .cate-123{background-position:-45px -172px}
.cate-nav__item .cate-7{background-position:-45px -134px}
.cate-nav__item .cate-125{background-position:-45px -58px}
.cate-nav__item .cate-124{background-position:-45px -77px}
.cate-nav__item .cate-9{background-position:-45px -115px}
.cate-nav__item .cate-8{background-position:-45px -152px}
.nav-level1{position:relative;padding:7px 0 2px 24px;border-top:1px solid #f8f8f8;width:175px}
.nav-level1--first{border-top:0;}
.nav-level1-inner{height:25px;overflow:hidden}
.nav-level1__label{display:inline-block;*display:inline;*zoom:1;height:22px;font-size:14px;font-weight:bold;color:#333;vertical-align:top}
.nav-level1__label:hover{text-decoration:none}
.nav-level1__item{display:inline;margin-right:6px;line-height:20px}
.nav-level1__item a{color:#666;vertical-align:top}
.nav-level1__item .bribe{color:#f76120}
.nav-level2{position:absolute;top:0;left:213px;padding:6px 30px 0 15px;width:285px;line-height:12px;background:#fff;border:1px solid #F08337;border-left:0;box-shadow:inset 4px 0 5px -2px #d9d9d9}
.nav-level2-label{font-size:14px;color:#333;line-height:28px;font-weight:bold}
.nav-level2-indication{position:absolute;width:3px;height:6px;left:12px;top:16px;background:url("../images/newicon.png") 0 -482px no-repeat}
.nav-active .nav-level2-indication{display:block}
.nav-level2-tile{margin-bottom:11px;padding-top:11px;border-top:1px dotted #dbdbdb;overflow:hidden;_zoom:1}
.nav-level2-tile--first{border-top:0;padding-top:0}
.nav-level2__label{color:#b0b0b0;font-weight:bold;line-height:28px}
.nav-level2__item{display:inline-block;*display:inline;*zoom:1;padding:0 10px;margin:6px 0;white-space:nowrap;border-left:1px solid #e0e0e0;color:#666;vertical-align:top}
.nav-level2-inner{margin-left:-10px}
.nav-level2-inner .bribe{color:#f76120}
.search-box{float:left;position:relative;margin:13px 0 0 70px;padding-top:24px;width:378px}
.search-box .search-suggest{top:61px}
.search-box__tabs{margin-top:-24px}
.search-box__tab{display:inline-block;*display:inline;font-size:12px;line-height:24px;padding:0 8px;*padding:6px 8px;cursor:pointer}
.search-box__tab--current{background:#F08338;color:#fff}
.search-box__hot{float:left;margin-top:2px;font-size:12px}
.search-box__hot a{padding:0 10px;border-right:0;height:18px;text-overflow:ellipsis;white-space:nowrap;max-width:60px;_width:50px;overflow:hidden;color:#666;display:inline-block;text-align:center;*zoom:1}
.search-box__hot a:hover{color:#F08337}
.search-box__form--no-hot{margin-top:23px}
.search-box__input{float:left;width:300px;margin:0;height:24px;line-height:24px;padding:3px 5px;border:3px solid #F08337;border-right:0;border-radius:0}
.search-box__input:focus{outline:0;background:#fff;-moz-box-shadow:inset 0 0 8px rgba(0,0,0,0.1);box-shadow:inset 0 0 8px rgba(0,0,0,0.1);;-moz-transition:all .2s linear;transition:all .2s linear}
.search-box__button{float:left;width:62px;margin:0;height:36px;line-height:36px;letter-spacing:2px;border:0 none;color:#FFF;background:#F08338;cursor:pointer;border-radius:0}
.search-box__button:hover{background:#F99956;-moz-transition:background .2s;transition:background .2s}
.sp-header-new-hd{background-image:url("../images/newicon.png");background-repeat: no-repeat;}
.site-mast__site-nav-inner .allMenu{font-family:微软雅黑;font-size:15px;font-weight:bold;line-height:39px;text-align:center;}
.site-mast__site-nav-inner .allMenu a{padding:0 12px;}
.site-mast__site-nav-inner .allMenu a:hover{}
.site-mast__site-nav-inner .sideMenu2 a{color:#fff;}
.site-mast__site-nav-inner .sideMenu2 {float:right;background:#f07521;border-left:1px solid #f07521;border-right:1px solid #f07521;font-family:微软雅黑;font-size:14px;font-weight:bold;height:39px;line-height:39px;padding:0 10px;}
.site-mast__site-nav-inner .sideMenu2 a.menu{color:fff;padding:0 8px}
.site-mast__site-nav-inner .vertical-bar {background:#f2f2f2;display:block;height:12px;position: absolute;right:-2px;top:5px;width:1px;z-index:94;}
