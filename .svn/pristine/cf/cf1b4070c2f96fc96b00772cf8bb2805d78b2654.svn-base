@charset "utf-8";
/* CSS Document */

body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td{margin:0;padding:0}
body{font:normal 14px/1.5 Tahoma,Helvetica,arial,sans-serif;color:#000;background-color:#f6f6f6}
table{border-collapse:collapse;border-spacing:0}
fieldset,img{border:0}
address,caption,cite,code,dfn,em,var{font-style:normal;font-weight:normal}
li{list-style:none}
caption,th{text-align:left}
h1,h2,h3,h4,h5,h6{font-size:100%}
q:before,q:after{content:''}
abbr,acronym{border:0;font-variant:normal}
sup{vertical-align:text-top}
sub{vertical-align:text-bottom}
input,textarea,select{font-family:inherit;font-size:inherit;*font-size:100%;font-weight:inherit}
legend{color:#000}
a{color:#F08337;text-decoration:none}
a:hover{text-decoration:underline}
table caption{padding:5px 8px;font-size:14px;font-weight:bold;color:#333}
span.money{font-family:arial}
span.required{color:red}
input.disabled{border:1px solid #ccc;background:#eee;color:#999;cursor:default}

.form{margin:0 0 10px 0}
.form--stack .btn{width:260px}

.cf{zoom:1}
.cf:after{content:'';display:block;height:0;overflow:hidden;visibility:hidden}
.tri{position:absolute;height:0;width:0;line-height:0;font-size:0;border:3px dashed transparent}
.clear{clear:both}
