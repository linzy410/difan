@charset "utf-8";
/* CSS Document */

#main{margin:0 auto;width:980px;padding: 10px 0 15px;}
.site-ms {float: left;overflow: hidden;padding:0;width: 980px;}

#filter{margin-bottom:10px;font-size:12px;line-height:19px;*zoom:1}
#filter .filter-label-list{padding-left:5em;_zoom:1;position:relative}
#filter .filter-label-list .label{top:0;left:0;width:5em;text-align:right;position:absolute}
#filter .inline-block-list .item{display:inline-block;*display:inline;*zoom:1}
#filter .sub-filter-wrapper .landmark-filter-wrapper{margin-top:8px;padding-left:0}
#filter .sub-filter-wrapper .landmark-filter-wrapper .label{padding-left:3px;width:6em}
#filter .filter-sortbar-outer-box{border:1px #d4d4d4 solid;background:#FFF}
#filter .filter-sortbar-outer-box--merge{border-top-color:#EEE}
#filter .filter-sortbar-outer-box .label{top:16px}
#filter .filter-section-wrapper{padding:0 15px}
#filter .filter-section-wrapper .filter-section{padding:12px 0 12px 65px;border-top:1px dotted #dbdbdb}
#filter .filter-main--attrs .filter-section{padding-left:50px}
#filter .filter-sortbar-outer-box .first-filter{border-top:0}
#filter .filter-section-wrapper .item{margin:4px 10px 4px 0;vertical-align:top}
#filter .filter-section-wrapper .item a{display:block;_display:inline;padding:0 4px;height:19px;_zoom:1}
#filter .filter-section-wrapper .item a span{margin-left:3px;font-size:10px;font-family:arial,sans-serif;color:#999}
#filter .filter-section-wrapper .item a:hover{border-radius:2px;text-decoration:none;color:#FFF;background-color:#f08337}
#filter .filter-section-wrapper .item a:hover span{color:#FFF}
#filter .filter-section-wrapper .current a{border-radius:2px;background-color:#f08337;color:#FFF}
#filter .filter-section-wrapper .current a:hover{cursor:pointer}
#filter .filter-section-wrapper .current a span{color:#FFF}
#filter .filter-section-wrapper .has-icon{padding-left:2em;width:3em;*zoom:1;background:url("../images/nav_icon.png") 0 0 no-repeat}
#filter .travel-filter-wrapper .has-icon{width:4em}
#filter .travel-filter-wrapper .has-icon,#filter .buziarea-filter-wrapper .has-icon,#filter .geo-filter-wrapper .has-icon{background-position:0 -20px}
#filter .dinnernum-filter-wrapper .has-icon{background-position:0 -40px}
#filter .dinnerprice-filter-wrapper .has-icon{background-position:0 -60px}
#filter .date-filter-wrapper .has-icon,#filter .time-filter-wrapper .has-icon{background-position:0 -100px}
#filter .room-filter-wrapper .has-icon{background-position:0 -120px}
#filter .subway-filter-wrapper .has-icon{background-position:0 -140px}
#filter .facility-filter-wrapper .has-icon{background-position:0 -160px}
#filter .brand-filter-wrapper .has-icon{background-position:0 -200px}
#filter .cinema-type-wrapper .has-icon{background-position:0 -220px}
#filter .destination-filter-wrapper .has-icon{background-position:0 -240px}
#filter .shoppingmall-filter-wrapper .has-icon{background-position:0 -260px}
#filter .sub-filter-wrapper{margin-top:5px;padding:5px 10px;background:#f7f7f7}
#filter .sub-filter-wrapper .label{top:4px;width:4em;color:#777}
#filter .sub-filter-wrapper .transfer-stop{color:#ee5238}
#filter .sub-filter-section{padding-left:0}

#filter .filter-section-wrapper .current .subway,#filter .filter-section-wrapper .item .subway:hover{background-position:-185px -19px}
#filter .filter-section-wrapper .item .briber,#filter .filter-section-wrapper .item .briber span{color:#ee5238}
#filter .filter-section-wrapper .current .briber,#filter .filter-section-wrapper .current .briber span{color:#FFF}
#filter .hot-tag-outer-box{border:1px solid #ffd6b9;border-bottom:0;padding:1px 1px 0;background-color:#ffefe4;)}
#filter .hot-tag-wrapper{padding:0 15px 0 80px;height:32px;background-color:#ffefe4;overflow:hidden;background-image:-moz-linear-gradient(bottom,#fdd8be 0,#fff9f4 15%);background-image:linear-gradient(bottom,#fdd8be 0,#fff9f4 15%)}
#filter .hot-tag-wrapper .label{top:7px;left:15px}
#filter .hot-tag-wrapper .item{margin:7px 12px 7px 0;padding:0 4px;height:18px;vertical-align:top}
#filter .hot-tag-wrapper .item:hover{border-radius:2px;text-decoration:none;color:#FFF;background-color:#f08337}
#filter .hot-tag-wrapper .hot{color:#ee5238}
#filter .filter-main--attrs .filter-section-wrapper .has-icon{background:none repeat;padding-left: 0;}
.filter-sect-list li{ display:inline-block;margin:4px 10px 4px 0;}
.filter-sect__title {left:0;position:absolute;text-align:right;top:16px;}

#filter .sort-bar{padding:0 15px;border-top:1px solid #EEE;height:37px;background:#f7f7f7;position:relative}
#filter .button-strip{margin:8px 10px 8px 0;font-size:12px;line-height:19px;text-align:center}
#filter .button-strip-item{border:1px solid #e8e8e8;padding:0 6px;background:#f9f9f9;color:#666;*cursor:pointer;_vertical-align:top}
#filter .button-strip-item-checked{background:#eeeded;color:#666;text-decoration:none}
#filter .button-strip-item:hover{background:#e0e0e0;text-decoration:none}
#filter .button-strip-item-checked:hover{background:#eeeded;box-shadow:none}
#filter .button-strip-item-left{border-top-left-radius:0;border-bottom-left-radius:0}
#filter .button-strip-item-right{margin-right:-1px;border-top-right-radius:0;border-bottom-right-radius:0}
#filter .button-strip-item .button-outer-box{width:4em}
#filter .middle-button .button-outer-box{width:5em}
#filter .large-button .button-outer-box{width:6em}
#filter .button-strip-item .button-content{line-height:19px}
#filter .button-strip-item .button-img{margin:6px 0 5px 6px;_margin:6px 0 -1px 6px;width:7px;height:8px;vertical-align:top}
#filter .button-strip-item-asc .sp-header-new{background-position:-85px -79px}
#filter .button-strip-item-desc .sp-header-new{background-position:-98px -79px}
#filter .button-strip-item-asc-active .sp-header-new{background-position:-112px -79px}
#filter .button-strip-item-desc-active .sp-header-new{background-position:-125px -79px}

#filter .checkbox{margin:9px 10px 8px 10px;padding-left:15px;color:#333;line-height:19px;text-decoration:none;text-align:left;background:url("../images/newicon.png") -237px -151px no-repeat;vertical-align:top;outline:0}
#filter .checkbox-nocheck:hover{text-decoration:none;background-position:-237px -128px;}
#filter .checkbox-checked{background-position:-237px -105px}
#filter .checkbox-disabled{background-position:-237px -174px;color:#AAA;cursor:default}
#filter .checkbox-checked-disabled{background-position:-237px -88px;cursor:default}
#filter .checkbox--hot{color:#ee5238}

#filter .around-filter-outer-box{margin-bottom:12px;border:1px solid #d4d4d4;background:#FFF}
#filter .around-filter-outer-box .filter-section{padding-left:5em}
#filter .around-filter-outer-box .first-filter{border-top:0}
#filter .around-filter-outer-box .label{top:16px;width:3em}
#filter .around-filter-outer-box .inline-link{display:inline-block;*display:inline;*zoom:1;vertical-align:top;line-height:27px}

.deal-tile{float:left;width:350px;border:1px solid #ddd;background:#fff;color:#999}
.deal-tile:hover .short-title{color:#333}
.deal-tile .w-link:hover .short-title{_color:#333}
.deal-tile .num{font-family:arial,sans-serif}
.deal-tile .deal-mark{top:-4px;left:10px}
.deal-tile__cover img{display:block}

.deal-tile__cover .trade-geo{z-index:124;bottom:0;position:absolute;width:318px;padding:5px 16px;max-height:166px;font-size:12px;line-height:22px;color:#ccc;opacity:0;filter:Alpha(opacity=0);overflow:hidden;background-color:#000;cursor:pointer;text-decoration:none!important;-moz-transition:opacity .5s ease-out;-webkit-transition:opacity .5s ease-out;transition:opacity .5s ease-out}
.deal-tile__cover .trade-geo em{color:#f76120}
.deal-tile__cover .geo-hover{opacity:.9;filter:Alpha(opacity=90)}
.deal-tile__cover .trade-geo:hover{opacity:.9;filter:Alpha(opacity=90)}
.deal-tile__cover .geo-title{font-weight:bold}
.deal-tile__cover .geo-title__icon{display:inline-block;width:13px;height:13px;padding-right:2px;vertical-align:-2px;background:url("../images/label-icons.png") 0 0 no-repeat;}

.deal-tile__title .w-link{display:block;height:64px;overflow:hidden;_cursor:pointer;-moz-transition:color .2s;-webkit-transition:color .2s;transition:color .2s}
.deal-tile__title .w-link:hover{text-decoration:none}
.deal-tile__title .xtitle,.deal-tile__title .short-title{display:block;-moz-transition:color .2s;transition:color .2s}
.deal-tile__title .xtitle{margin-bottom:4px;color:#333;font-size:16px;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;_width:306px}

.deal-tile__title em{font-weight:inherit;color:#f76120}

.deal-tile__detail .price{margin-right:10px;font-size:26px;color:#f76120}
.deal-tile__detail .price .price-label--wedding{font-size:16px;margin-right:6px}

.deal-tile__detail .value{font-size:12px}
.deal-tile__detail .value del,.deal-tile__detail .value .num--wedding{padding-left:6px}
.deal-tile__extra{padding:1px;border-top:1px #f3f3f3 solid}
.deal-tile__extra .extra-inner{height:33px;padding:0 15px;font-size:12px;background-color:#fafafa}
.deal-tile__extra .sales{float:left;height:33px;line-height:33px}
.deal-tile__extra .sales .num{margin-left:2px;color:#f76120;font-weight:normal}
.deal-tile__extra .rate-info{float:right;height:33px;line-height:33px;cursor:pointer}
.deal-tile__extra .rate-info:hover,.deal-tile__extra .rate-info:hover .rate-info__count{color:#F08337;text-decoration:none}
.deal-tile__extra .rate-info__bar{margin-right:5px;*margin-right:8px;vertical-align:-1px;*vertical-align:1px}
.deal-tile__extra .rate-info__count{display:inline-block;color:#949494}
.deal-tile__extra .rate-info--noreviews{cursor:default}
.deal-tile__extra .rate-info--noreviews:hover{color:inherit}
.deal-tile__extra .marker{float:right;top:10px;width:10px;height:13px;overflow:hidden}
.deal-tile__extra .maker--distance{background-position:-54px -77px}
.deal-tile__extra .sp-around-icon{background-position:0 -62px}
.deal-tile__extra .subway-distance,.deal-tile__extra .around-distance{float:right;padding-left:5px;font-size:12px;line-height:33px}
.deal-tile__extra .around-distance em{padding:0 3px;font-weight:bold;color:#fd6b44}

.mall{*float:left;width:720px;clear:left}
.mall .brand-tile,.mall .deal-tile{margin-bottom:16px;box-shadow:0 2px 0 0 #ededed}
.mall .brand-tile:hover,.mall .deal-tile:hover{border-bottom-color:#F08337;box-shadow:inset 0 -1px 0 0 #F08337}
.mall .deal-tile--even{float:right}.mall .page-nav-wrapper{clear:both;padding-top:20px;text-align:center}
.pg-nav .page-nav-wrapper{padding-top:24px}

.side-extension__item--movie{background-position:0 -116px}
.side-extension__item--hotel{background-position:0 -232px}
.side-extension--link .side-extension__item{margin:0;padding:40px 0 20px 40px;background-image:url("../images/sidebar_icon.png");background-repeat:no-repeat;color:#666}
.side-extension--link .side-extension__item:hover{text-decoration:none;color:#F08337}

.stick-qrcode{position:fixed;margin-left:500px;}
.stick-qrcode .stick-qrcode__pos{top:128px;left:17px;width:74px;height:74px}
.stick-qrcode .index-xiaomei{display:inline-block;width:114px;height:255px;vertical-align:top;outline:0;background-position:0 -1040px}
.stick-qrcode .index-xiaomei .close{top:110px;right:6px;width:20px;height:20px}
.stick-qrcode .stick-qrcode-category{background-position:0 -367px}

.deals-async-loading{margin-bottom:16px;padding:10px 0;height:21px;background-color:#FFF;border:1px solid #ddd;float:left;width:100%}
.deals-async-loading__tips{margin:0 auto;width:90px;font-size:14px;color:#999}
.deals-async-loading__tips i{float:right;width:44px;height:21px;background-image:url("../images/loading.gif");background-position:center center;background-repeat:no-repeat}
