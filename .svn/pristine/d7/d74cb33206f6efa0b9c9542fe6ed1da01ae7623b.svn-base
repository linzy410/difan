@charset "utf-8";
/* CSS Document */

.side-extension--history .side-extension__item{width:198px}
.side-extension--multiple{padding:0 20px 1px}
.side-extension{margin-bottom:16px;padding:10px 20px;border:1px solid #DDD;box-shadow:0 2px 0 0 #ededed;background-color:#FFF;font-size:12px;*zoom:1}

.deal-tile--reco .w-link{width:198px;height:auto}
.deal-tile--reco .w-link:hover .short-title{color:#F08337}
.deal-tile__cover{display:block;_zoom:1;text-decoration:none;position:relative}
.deal-tile__cover:hover{text-decoration:none}

.deal-tile{float:left;width:350px;border:1px solid #ddd;background:#fff;color:#999}
.deal-tile:hover .short-title{color:#333}

.top-reco-deals .deal-tile--reco{float:none}
.deal-tile__title{margin-top:15px;padding:0 16px;font-family:'宋体',sans-serif}
.deal-tile__title .short-title{line-height:18px;color:#999;font-weight:normal}
.deal-tile--reco{border:0;width:198px;height:195px;font-size:12px}
.deal-tile--reco:hover{box-shadow:none}
.deal-tile--reco .deal-tile__title{margin-top:10px;padding:0}
.deal-tile--reco .short-title{color:#666}
.deal-tile--reco .deal-tile__detail{padding:0}
.deal-tile__detail{padding:0 16px 2px}
.deal-tile--reco .price{font-size:22px}
.deal-tile__detail .num{font-family:arial,sans-serif}
.deal-tile__detail .price strong{margin-left:2px}
.side-extension--multiple {padding: 0 20px 1px;}
.side-extension--download .side-extension__item{margin:0;padding:0;width:198px;height:115px;background-image:url("../images/sidebar_icon.png");background-repeat:no-repeat}
.side-extension .deal-tile--reco{height:auto}

.side-extension__item--client{background-position:0 0}
.deal-tile--reco .deal-rank{position:absolute;top:-2px;left:0;padding:0 2px 2px 0;width:16px;height:16px;line-height:16px;color:#fff;text-align:center;background:url("../images/label-icons.png") no-repeat;background-position:-20px 0}
.site-sidebar{float:right;width:240px;_display:inline}
.side-extension h3{padding:3px 0 13px;font-size:16px}
.side-extension img{display:block}
.side-extension__item{position:relative;display:block;border-bottom:1px dotted #DDD;margin-bottom:20px;padding-bottom:13px}
.side-extension__item--last{border-bottom:0;margin-bottom:0}
.side-extension--multiple{padding:0 20px 1px}
.reco-sliders-w .pre-next .sp-slide--previous{background-position: -206px 0;}
.side-extension__item--client{background-position:0 0}
.side-extension__item--movie{background-position:0 -116px}
.side-extension__item--hotel{background-position:0 -232px}
.side-extension--link .side-extension__item:hover{text-decoration:none;color:#F08337}
.side-extension--link .description{display:block;color:#666}
.side-extension__item--job{background-position:0 -348px}
.side-extension__item--feedback{background-position:0 -425px}
.side-extension__item--business{background-position:0 -501px}
.side-extension--single{padding:0}


.side-extension--history .clear-history {float: right;font-size: 12px;font-weight: normal;line-height: 24px;}
.side-extension--history .history-list h5 {font-size: 12px;font-weight: normal;height: 32px;line-height: 16px;overflow: hidden;}
.side-extension--history .no-history{height:60px;width:198px;line-height:60px;color:#666;text-align:center}

.side-extension--history .history-list__item .old-price {color: #999999;}
.side-extension--history .history-list__item .price {color: #F76120;font: bold 12px/12px arial,sans-serif;padding-right: 5px;}
.side-extension--history .history-list__item img {float: left;margin: 2px 10px 0 0;}
.side-extension--history .history-list__item a {color: #666666;}
.side-extension--history .history-list__item a:hover{color:#F08337;text-decoration:none}

.side-extension--history .history-list__item{padding:18px 0;font-size:12px;color:#666;zoom:1;border-bottom:1px dotted #DDD}
.side-extension--history .history-list__item--first{padding-top:0}
.side-extension--history .history-list__item--last{border:0;padding-bottom:0}

.side-extension--history .history-list__item p{padding-top:4px}

#sidebar .side-single{padding:0 15px;margin-bottom:10px;border:1px solid #d4d4d4;background:#fff}
#sidebar .side-single .inner-side{padding:0}
